{
  "name": "noa-examples",
  "version": "0.0.3",
  "description": "Starter examples of game worlds using the noa voxel engine",
  "scripts": {
    "start": "cd src/hello-world && webpack-dev-server",
    "test": "cd src/test && webpack-dev-server",
    "build1": "cd src/test && webpack --env.production",
    "build2": "cd src/hello-world && webpack --env.production",
    "build": "npm run build1 && npm run build2"
  },
  "keywords": [
    "noa-engine",
    "noa",
    "voxel",
    "game",
    "engine",
    "example"
  ],
  "author": "Andy Hall",
  "license": "ISC",
  "dependencies": {
    "@babylonjs/core": "^4.0.3",
    "@babylonjs/inspector": "^4.0.3",
    "noa-engine": "github:andyhall/noa#develop",
    "voxel-crunch": "^0.2.1"
  },
  "devDependencies": {
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
